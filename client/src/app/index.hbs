<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>netIOT Connectivity Kit â€“ Sensor to Cloud</title>
  <base href="/">

  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="icon" type="image/x-icon" href="favicon.ico">
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/hammer.js/2.0.8/hammer.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.3/Chart.bundle.min.js"></script>
  {{#if htmlWebpackPlugin.options.isBuild}}
    {{#each htmlWebpackPlugin.files.chunks}}
      <script src="{{entry}}" defer></script>
    {{/each}}
  {{/if}}
  {{#if isIde}}
    <!-- 1. Load libraries -->
    <!-- Polyfill(s) for older browsers -->
    <script src="../../../node_modules/systemjs/dist/system.src.js"></script>
    <!--<script src="../../../node_modules/chart.js/dist/Chart.bundle.min.js"></script>-->

    <!--   <link rel="stylesheet" href="app/styles.css"> -->
    <!--<link rel="stylesheet" href="app/assets/styles/style.css"/>-->

    <!-- 2. Configure SystemJS -->
    <script src="src/app/systemjs.config.js"></script>
    <script>
      window.systemJsBaseUrl = '';
      System.import('src/app/polyfills.js')
        .then(function () {
          return System.import('src/main.js');
        })
        .then(function () {
          console.log('Loaded javascript files via systemjs -> start initialization of application now...');
        })
        .catch(function (err) {
          console.error(err);
        });
    </script>
  {{/if}}

</head>
<body>
