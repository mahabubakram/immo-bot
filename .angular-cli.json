{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "version": "1.0.0-beta.32.3",
    "name": "todo-app"
  },
  "apps": [
    {
      "root": "client/src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "app/polyfills.ts",
      "test": "test.ts",
      "tsconfig": "../tsconfig.json",
      "prefix": "client/src",
      "styles": [
        "app/styles.css"
      ],
      "scripts": [],
      "environmentSource": "client/src/environments/environment.ts",
      "environments": {
        "dev": "client/src/environments/environment.ts",
        "prod": "client/src/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "files": "client/src/**/*.ts",
      "project": "./tsconfig.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
